model Type {
  id          Int                  @id
  name        String
  description String?
  group_id    Int
  published   Boolean              @default(true)
  volume      Float?
  capacity    Float?
  mass        Float?
  icon_id     Int?
  created_at  DateTime             @default(now())
  updated_at  DateTime             @updatedAt
  attributes  TypeDogmaAttribute[]
  effects     TypeDogmaEffect[]

  @@index([group_id])
  @@map("types")
}
